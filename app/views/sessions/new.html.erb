<h1>Log In:</h1>

<%= form_with url: login_path do |form| %>

  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  
<div py-2 px-4 bg-blue-200>
  <%= form.submit "Log in" %>
</div>
  
  

<% end %>